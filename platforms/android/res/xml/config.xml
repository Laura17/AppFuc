<?xml version='1.0' encoding='utf-8'?>
<widget id="com.kmedia.fucs" version="1.1.9" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="Notification">
        <param name="android-package" value="org.apache.cordova.dialogs.Notification" />
    </feature>
    <feature name="PushPlugin">
        <param name="android-package" value="com.plugin.gcm.PushPlugin" />
    </feature>
    <name>fucsapp</name>
    <description>A sample Apache Cordova application that responds to the deviceready event.</description>
    <author email="mposadar@gmail.com" href="https://mposadar.wordpress.com/about/">Miguel Angel Posada</author>
    <content src="index.html" />
    <access origin="*" />
</widget>
